{
  "name": "Modal Blocker",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Modal Blocker"
  },
  "permissions": [
    "background",
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "19": "icons/icon-19.png",
    "32": "icons/icon-32.png",
    "38": "icons/icon-38.png",
    "128": "icons/icon-128.png"
  }
}
